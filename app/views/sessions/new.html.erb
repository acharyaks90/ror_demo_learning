<h1>Login Page
</h1>
<%= form_with( scope: :session, url: login_path, local: true) do |form| %>
    <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
    
    <div class="actions">
      <%= form.submit "Log in" %>
    </div>
  <% end %>